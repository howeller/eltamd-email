<table class="cta-wrapper" align="center" width="{{root.global.cta.width}}" height="{{root.global.cta.height}}" style="width:{{root.global.cta.width}}px;max-width:{{root.global.cta.width}}px;" border="0" cellpadding="0" cellspacing="0" role="presentation">
	<tr>
		<td class="cta-btn vert-center" align="center" valign="middle">
			<a href="{{{cta.url}}}" class="cta-txt"><span class="cta-txt">{{{cta.text}}}</span></a>
		</td>
	</tr>
</table>
